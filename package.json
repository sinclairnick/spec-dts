{
  "name": "spec-dts",
  "description": "OpenAPI type inference without the codegen",
  "version": "0.1.0",
  "author": {
    "name": "Nick Sinclair",
    "email": "nicksinclair@xtra.co.nz",
    "url": "https://github.com/sinclairnick"
  },
  "peerDependencies": {
    "openapi3-ts": "^4.4.0"
  },
  "devDependencies": {
    "openapi3-ts": "^4.4.0",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2",
    "vitest": "^1.2.2"
  },
  "keywords": [
    "openapi",
    "spec",
    "no-codegen",
    "oas",
    "typescript",
    "inference"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "private": false,
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit",
    "publish-pkg": "pnpm run build && pnpm publish --access=public --no-git-checks",
    "test": "vitest"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts"
}
